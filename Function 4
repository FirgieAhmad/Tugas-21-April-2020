using namespace std;

//Membuat Function tanpa Parameter tetapi dengan nilai kembalian
int fungsi () {
		int a = 50, b = 100;
		return a * b;
}

//ini adalah function utama
int main (){
	//pemanggilan function 
	cout << fungsi ();
}
