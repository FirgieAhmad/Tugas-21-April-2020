using namespace std;

//Membuat Function Parameter dan Nilai Kembalian
int pembagian ( int a, int b){
	return a / b;
}
int main (){
	cout << "ini adalah Function Parameter dan Nilai Kembalian" << endl;
	cout << pembagian ( 200, 20);
}
